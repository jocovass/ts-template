    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tsconfig/node22": "^22.0.2",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/eslint-plugin": "^1.2.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-perfectionist": "^4.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.1",
    "prettier": "^3.5.3",
    "tsx": "^4.20.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "vitest": "^3.2.3"
